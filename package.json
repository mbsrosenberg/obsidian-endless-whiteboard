  "license": "MIT",
  "devDependencies": {
    "@types/fabric": "^4.5.15",
    "@types/node": "^16.18.122",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "^1.7.2",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "fabric": "^5.5.1",
    "tesseract.js": "^4.1.4"
  }
